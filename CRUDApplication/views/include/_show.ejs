<% results.forEach(user => { %>
    <tr>
        <td><%= user.Name %></td>
        <td><%= user.email %></td>
        <td><%= user.gender %></td>
        <td><%= user.status %></td>
        <td>
            <a href="/edit/<%= user.Name %>" class="btn btn-primary">
                <i class="fas fa-edit"></i> Edit
            </a>
            <button class="btn btn-danger" onclick="deleteUser('<%= user.Name %>')">
                <i class="fas fa-trash-alt"></i> Delete
            </button>
        </td>
    </tr>
<% }) %>
